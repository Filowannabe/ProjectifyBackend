{
  "name": "projectify",
  "version": "1.0.0",
  "engines": {
    "node": "16.x"
  },
  "description": "App for AdsForGood",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev src/index.ts",
    "migrate": "cross-env NODE_ENV=development npx knex migrate:latest --knexfile src/Config/knexfile.ts",
    "migrate:down": "cross-env NODE_ENV=development npx knex migrate:down  --knexfile src/Config/knexfile.ts",
    "migrate:rollback:all": "cross-env NODE_ENV=development npx knex migrate:rollback --all  --knexfile src/Config/knexfile.ts",
    "test": "cross-env NODE_ENV=test jest --verbose  --detectOpenHandles --silent",
    "test:watch": "cross-env NODE_ENV=test npm run test -- --watch"
  },
  "author": "Felipe",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.11",
    "log4js": "^6.3.0",
    "luxon": "^2.3.0",
    "objection": "^2.2.17",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/knex": "^0.16.1",
    "@types/node": "^16.11.1",
    "@types/luxon": "^2.0.9",
    "@types/pg": "^8.6.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.0.1",
    "jest": "^27.3.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "/dist/"
    ]
  }
}
